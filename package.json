{
  "devDependencies": {
    "cypress": "^9"
  },
  "scripts": {
    "test": "./node_modules/.bin/cypress run --browser chrome --env host=host  --config video=false --spec **/*.spec.js",
    "test:watch": "./node_modules/.bin/cypress open  --env host='host'",
    "test:dashboard": "./node_modules/.bin/cypress run --browser chrome --env host='host' --record --key keyRecord --config video=true --spec **/*.spec.js"
  },
  "dependencies": {}
}
